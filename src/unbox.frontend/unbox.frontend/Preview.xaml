<Window x:Class="unbox.frontend.Preview"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:unbox.frontend"
        xmlns:usercontrols="clr-namespace:unbox.frontend.usercontrols"
        mc:Ignorable="d"
        Title="Preview" Height="450" Width="450" Topmost="True" WindowStyle="None" AllowsTransparency="True"
       >
    <Window.Background>
        <LinearGradientBrush StartPoint="0,0" EndPoint="50,0" MappingMode="Absolute">
            <LinearGradientBrush.GradientStops>
                <GradientStop Offset="0" Color="#00ffffff" />
                <GradientStop Offset="1" Color="#ffffff" />
            </LinearGradientBrush.GradientStops>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <usercontrols:TimeSlotPlan Margin="50,0,0,0" />
        
    </Grid>
</Window>
